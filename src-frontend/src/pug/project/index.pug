html(lang="en")
    head
        title LateUnicorn.com
        meta(charset="utf-8")
        meta(name="viewport", content="width=device-width, initial-scale=1, shrink-to-fit=no")
        link(rel="preconnect", href="https://fonts.gstatic.com")
        link(href="https://fonts.cdnfonts.com/css/satoshi?styles=135009,135005,135007,135002,135000", rel="stylesheet")
        link(rel="stylesheet", href="/css/tailwind/tailwind.min.css")
        link(rel="icon", type="image/png", sizes="32x32", href="/favicon.png")
        script(defer, src="https://cdn.jsdelivr.net/npm/@supabase/supabase-js@2")
        script(defer, src="/js/main.js")
        script(defer, src="/js/project.js")
    body(class="antialiased bg-body text-body font-body bg-gray-900")
        div(class="")
            include ../partials/navbar.pug

            section(class="pb-8 overflow-hidden")
                div(class="container mx-auto px-4")
                    div(class="py-16 px-8 bg-gray-800 rounded-3xl")
                        div(id="project-spinner", class="w-full py-20 text-center")
                            img(class="w-20 h-20 inline-block animate-spin", src="/images/spinner.svg", alt="")

                        div(id="project-details", class="max-w-7xl mx-auto hidden")
                            div(class="mb-10 md:max-w-4xl mx-auto text-center")
                                h2(data-name="", class="font-heading text-4xl md:text-5xl lg:text-6xl text-white font-black")
                                div(data-project_id="", class="mb-6 text-xl text-gray-500 font-black")
                            ul(id="category-list", class="flex flex-wrap justify-center -m-3 mb-8")
                            div(class="flex flex-wrap -m-8")
                                div(class="w-full md:flex-1 p-8")
                                    div(class="flex flex-wrap -m-3")
                                        div(class="w-full p-3")
                                            div(class="p-10 bg-gray-900 rounded-3xl")
                                                div(class="flex flex-wrap -m-2")
                                                    div(class="w-full md:w-1/2 p-2")
                                                        h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Project description:
                                                        p(data-description="", class="text-gray-100 text-xl font-bold")
                                                    div(class="w-full flex flex-wrap md:w-1/2 md:pl-8")
                                                        div(class="w-full md:w-1/2 p-2")
                                                            h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Total users:
                                                            p(data-total_users="", class="text-gray-100 text-xl font-bold")
                                                        div(class="w-full md:w-1/2 p-2")
                                                            h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Total revenue:
                                                            p(data-total_revenue="", class="text-gray-100 text-xl font-bold")
                                                        div(class="w-full md:w-1/2 p-2")
                                                            h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Project lifespan:
                                                            p(data-lifetime="", class="text-gray-100 text-xl font-bold")
                                                    div(class="w-full md:w-1/2 p-2")
                                                        h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Founder(s):
                                                        p(data-founders="", class="text-gray-100 text-xl font-bold")
                                        div(class="w-full p-3")
                                            div(class="p-10 bg-gray-900 rounded-3xl")
                                                div(class="flex flex-wrap -m-2")
                                                    div(class="w-full md:w-1/2 p-2")
                                                        h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Core features:
                                                        p(data-features="", class="text-gray-100 text-xl font-bold")
                                                    div(class="w-full md:w-1/2 md:pl-8")
                                                        div(class="w-full p-2")
                                                            h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Core technologies:
                                                            p(data-technologies="", class="text-gray-100 text-xl font-bold")
                                        div(class="w-full p-3")
                                            div(class="p-10 bg-gray-900 rounded-3xl")
                                                div(class="flex flex-wrap -m-2")
                                                    div(class="w-full md:w-1/2 p-2")
                                                        h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Point of failures:
                                                        p(data-failures="", class="text-gray-100 text-xl font-bold")
                                                    div(class="w-full md:w-1/2 md:pl-8")
                                                        div(class="w-full p-2")
                                                            h2(class="font-heading mb-2 text-2xl text-gray-600 font-black") Main learnings:
                                                            p(data-learnings="", class="text-gray-100 text-xl font-bold")
                                        div(id="speculation-section", class="w-full p-3 hidden")
                                            div(class="p-10 bg-gray-900 rounded-3xl")
                                                div(class="-m-2")
                                                    div(class="w-full p-2 relative")
                                                        svg(class="absolute -start-5 w-2/3 p-4 text-gray-100 dark:text-neutral-700", width="18", height="18", viewBox="0 0 16 16", fill="none", xmlns="http://www.w3.org/2000/svg", aria-hidden="true", hidden)
